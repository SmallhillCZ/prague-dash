{
  "dockerComposeFile": "docker-compose.yml",
  "service": "devcontainer",
  "postCreateCommand": "./.devcontainer/post-create.sh",
  "workspaceFolder": "/workspaces",
  "features": {
    "ghcr.io/devcontainers/features/java:1": {}
  },
  "forwardPorts": [
    4200,
    3000,
    9229,
    "pgweb:8081",
    "db:5432"
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "esbenp.prettier-vscode",
        "angular.ng-template"
      ]
    }
  }
}